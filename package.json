  "scripts": {
    "start": "react-scripts --max_old_space_size=8192 start",
    "build": "react-scripts build && cp -r build h5pextend && mv h5pextend build/h5pextend",
    "test": "react-scripts test",
    "syncapi": "node ./scripts/syncapi.js",
    "syncLang": "node ./scripts/syncLang",
    "syncGraphql": "graphql-codegen -c ./scripts/graphqlGenConfig.yml",
    "syncH5p": "node ./scripts/syncH5pVersion.js && cnpm i h5p",
    "lint": "eslint --max-warnings=0 --ext .jsx,.js,.ts,.tsx src/",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "public/h5p/**/*": [
      "node ./scripts/syncH5pVersion.js",
      "git add",
      "git add public/h5p/package.json"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{css,scss,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint",
      "commit-msg": "yarn commitlint --edit $1"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/generator": "^7.11.6",
    "@babel/parser": "^7.11.5",
    "@babel/template": "^7.10.4",
    "@babel/traverse": "^7.11.5",
